<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <_TizenNETFrameworkReferenceVersion>9.0.0-pre1</_TizenNETFrameworkReferenceVersion>
  </PropertyGroup>

  <ItemGroup>
    <KnownFrameworkReference
      Include="Tizen.NET"
      TargetFramework="net6.0"
      RuntimeFrameworkName="Tizen.NET"
      DefaultRuntimeFrameworkVersion="$(_TizenNETFrameworkReferenceVersion)"
      LatestRuntimeFrameworkVersion="$(_TizenNETFrameworkReferenceVersion)"
      TargetingPackName="Tizen.NET.Ref"
      TargetingPackVersion="$(_TizenNETFrameworkReferenceVersion)"
      RuntimePackNamePatterns="Tizen.NET.Runtime.**RID**"
      RuntimePackRuntimeIdentifiers="tizen-arm;tizen-x86"
      Profile="Tizen"
      IsTrimmable="true"
    />
  </ItemGroup>

</Project>
