<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<AfterMicrosoftNETSdkTargets>$(AfterMicrosoftNETSdkTargets);$(MSBuildThisFileDirectory)Tizen.NET.Sdk.Workload.After.targets</AfterMicrosoftNETSdkTargets>
	</PropertyGroup>

  <ItemGroup Condition=" '$(DisableImplicitFrameworkReferences)' != 'true' ">
    <FrameworkReference
        Include="Tizen.NET"
        IsImplicitlyDefined="true"
        Pack="false"
        PrivateAssets="All"
    />
  </ItemGroup>

  <Import Project="Tizen.NET.Sdk.Versions.targets" />
  <Import Project="Tizen.NET.Sdk.SupportedPlatforms.targets" />

</Project>
