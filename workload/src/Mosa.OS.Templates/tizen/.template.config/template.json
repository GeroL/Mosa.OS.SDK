{
  "$schema": "http://json.schemastore.org/template",
  "author": "Samsung Electronics",
  "classifications": [ "Tizen" ],
  "identity": "Samsung.Tizen.TizenApp",
  "name": "Tizen .NET Application",
  "description": "A project for creating an Tizen .NET application",
  "shortName": "tizen",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "TizenApp1",
  "preferNameDirectory": true,
  "primaryOutputs": [
    { "path": "TizenApp1.csproj" }
  ],
  "symbols": {
    "packageName": {
      "type": "parameter",
      "description": "Overrides the package name in the tizen-manifest.xml",
      "datatype": "string",
      "replaces": "com.companyname.TizenApp1"
    }
  },
  "defaultName": "TizenApp1"
}