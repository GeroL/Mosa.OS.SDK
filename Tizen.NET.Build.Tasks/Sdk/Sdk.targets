<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>

  <ImportGroup>
    <Import Project="$(MSBuildThisFileDirectory)..\build\Tizen.NET.Sdk.Inference.targets" />
    <Import Project="$(MSBuildThisFileDirectory)..\build\Tizen.NET.Sdk.ImplicitPackages.targets" />
  </ImportGroup>

  <Import Project="Sdk.targets" Sdk="Microsoft.NET.Sdk"/>

  <!--
    Special condition _IsTizenNetSdkTargetsImported to prevent importing targets twice
  -->
  <ImportGroup Condition="'$(_IsTizenNetSdkTargetsImported)' != 'true'">
    <Import Project="$(MSBuildThisFileDirectory)..\build\Tizen.NET.Sdk.targets" />
  </ImportGroup>

</Project>
